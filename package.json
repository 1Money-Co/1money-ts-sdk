{
  "name": "@1money/ts-sdk",
  "version": "0.3.6",
  "description": "js sdk for 1money co",
  "main": "lib/index.js",
  "module": "es/index.js",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./lib/index.js",
      "types": "./es/index.d.ts"
    },
    "./api": {
      "import": "./es/api/index.js",
      "require": "./lib/api/index.js",
      "types": "./es/api/index.d.ts"
    },
    "./client": {
      "import": "./es/client/index.js",
      "require": "./lib/client/index.js",
      "types": "./es/client/index.d.ts"
    },
    "./utils": {
      "import": "./es/utils/index.js",
      "require": "./lib/utils/index.js",
      "types": "./es/utils/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "unpkg": "umd/1money-ts-sdk.min.js",
  "scripts": {
    "start": "omni dev",
    "dev": "omni dev",
    "test": "nyc mocha --config .mocharc.js",
    "lint": "npm run lint:prettier && npm run lint:es",
    "lint:fix": "npm run lint:prettier_fix && npm run lint:es_fix",
    "lint:es": "eslint src/ --ext .ts --ext .tsx",
    "lint:es_fix": "eslint src/ --ext .ts --ext .tsx --fix",
    "lint:prettier": "prettier --check src/",
    "lint:prettier_fix": "prettier --write src/",
    "lint:commit": "commitlint -e $HUSKY_GIT_PARAMS",
    "new": "omni new",
    "build": "omni build",
    "build:demo": "dumi build",
    "release": "omni release",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:es_fix",
      "npm run lint:prettier_fix"
    ]
  },
  "keywords": [],
  "author": "",
  "resolutions": {
    "@mapbox/hast-util-to-jsx": "1.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.25.9",
    "@babel/plugin-transform-runtime": "~7.25.9",
    "@babel/preset-env": "~7.25.9",
    "@babel/preset-typescript": "~7.25.9",
    "@babel/runtime-corejs3": "~7.25.9",
    "@commitlint/cli": "8.3.5",
    "@omni-door/cli": "~3.1.0",
    "@rollup/plugin-alias": "~5.1.1",
    "@rollup/plugin-babel": "~6.0.4",
    "@rollup/plugin-commonjs": "~28.0.3",
    "@rollup/plugin-json": "~6.1.0",
    "@rollup/plugin-node-resolve": "~16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/chai": "4.3.17",
    "@types/mocha": "10.0.7",
    "@types/node": "~18.19.39",
    "@typescript-eslint/eslint-plugin": "~8.30.0",
    "@typescript-eslint/parser": "~8.30.0",
    "chai": "4.5.0",
    "del": "5.1.0",
    "detect-port": "1.3.0",
    "dumi": "2.4.7",
    "eslint": "~8.57.1",
    "eslint-config-prettier": "~10.0.1",
    "eslint-plugin-prettier": "~5.2.3",
    "husky": "9.1.7",
    "ip": "1.1.5",
    "lint-staged": "15.4.3",
    "mocha": "10.7.0",
    "nyc": "17.0.0",
    "prettier": "~3.5.0",
    "prettier-eslint": "^16.3.0",
    "rollup": "~4.40.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "ts-loader": "9.5.1",
    "ts-patch": "~3.2.1",
    "tsconfig-paths": "~3.9.0",
    "tslib": "^2.6.3",
    "tsx": "^4.17.0",
    "typescript": "~5.8.3",
    "typescript-transform-paths": "~3.5.5",
    "webpack": "~5.93.0"
  },
  "dependencies": {
    "axios": "~1.7.3",
    "bignumber.js": "~9.1.2",
    "chalk": "~4.1.2",
    "dayjs": "~1.11.13",
    "ethers": "^6.13.5"
  }
}
